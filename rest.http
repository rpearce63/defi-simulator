POST https://api.morpho.org/graphql 
Content-Type: application/json
X-REQUEST-TYPE: GraphQL 

query VaultPosition() {
  vaultPosition(
  userAddress: $userAddress, 
  vaultAddress: $vaultAddress) {
    state {
      assets
      assetsUsd
    }
  }
}